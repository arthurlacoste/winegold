{
  "name": "winegold",
  "version": "0.0.1",
  "description": "A file processor & converter",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "xo && electron . -t",
    "icon": "electron . -t",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/arthurlacoste/winegold",
  "keywords": [
    "converter",
    "electron",
    "file processing"
  ],
  "build": {
    "extraResources": [
      {
        "from": "scripts",
        "to": "scripts"
      }
    ],
    "mac": {
      "target": "dmg",
      "category": "public.app-category.productivity",
      "icon": "icons/mac/icon.icns"
    },
    "win": {
      "icon": "icons/win/icon.ico"
    }
  },
  "author": "Arthur Lacoste",
  "license": "CC-BY-SA-4.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-builder": "^19.41.0",
    "electron-icon-maker": "0.0.4",
    "electron-reload": "^1.2.2",
    "jest": "^21.2.1",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "bytes": "^3.0.0",
    "child-process-fork-string": "^1.0.0",
    "classes": "^0.3.0",
    "electron-directory": "^0.0.3",
    "electron-is-dev": "^0.3.0",
    "electron-store": "^1.3.0",
    "fs-extra": "^4.0.2",
    "id.log": "^1.1.0",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "semantic-ui-css": "^2.2.12"
  },
  "xo": {
    "global": ["$","document","launchScript","loadList","addItem"]
  }
}
