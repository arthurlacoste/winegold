{
  "name": "winegold",
  "version": "0.0.1",
  "description": "A file processor & converter",
  "main": "build/main.js",
  "scripts": {
    "start": "npm run cp && electron .",
    "test": "xo --fix && npm run cp",
    "t": "electron . -t",
    "dist": "electron-builder",
    "purge": "electron src/test/purge.js",
    "build": "node ./bin/package.js",
    "cp": "node ./bin/cp",
    "watch": "nodemon --exec \"xo --fix && npm run start\" --ext js,css --ignore build/ --ignore dist/"
  },
  "repository": "https://github.com/arthurlacoste/winegold",
  "keywords": [
    "converter",
    "electron",
    "file processing"
  ],
  "author": "Arthur Lacoste",
  "license": "CC-BY-SA-4.0",
  "devDependencies": {
    "electron": "~1.7.8",
    "electron-icon-maker": "0.0.4",
    "electron-reload": "^1.2.2",
    "electron-osx-sign": "^0.4.3",
    "electron-packager": "~8.5.1",
    "electron-winstaller": "~2.5.2",
    "minimist": "^1.2.0",
    "jest": "^21.2.1",
    "xo": "^0.18.2",
    "nodemon": "^1.10.2"
  },
  "dependencies": {
    "application-config": "^1.0.1",
    "arch": "^2.1.0",
    "async": "^2.5.0",
    "bytes": "^3.0.0",
    "child-process-fork-string": "^1.0.0",
    "classes": "^0.3.0",
    "cross-zip": "^2.1.5",
    "drag-drop": "^2.13.2",
    "electron-directory": "^0.0.3",
    "electron-is-dev": "^0.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^1.3.0",
    "fs-extra": "^4.0.2",
    "id.log": "^2.0.6",
    "jquery": "^3.2.1",
    "js-yaml": "^3.10.0",
    "mkdirp": "^0.5.1",
    "nobin-debian-installer": "0.0.10",
    "parse-filepath": "^1.0.1",
    "plist": "^2.1.0",
    "rimraf": "^2.6.2",
    "run-series": "^1.1.4",
    "semantic-ui-offline": "^2.2.13",
    "tampax": "*",
    "tree-kill": "^1.2.0",
    "walk-sync": "^0.3.2",
    "winreg": "^1.2.4"
  },
  "xo": {
    "global": [
      "$",
      "document",
      "window",
      "loadList",
      "addItem"
    ],
    "rules": {
      "unicorn/no-process-exit": 0,
      "curly": [
        "error",
        "multi-line"
      ],
      "no-eval": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "assets"
          ]
        }
      ]
    }
  },
  "optionalDependencies": {
    "appdmg": "^0.5.1"
  }
}
